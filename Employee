package f254;
public class Employee {
  private int empId; 
  private String name;
  private String dept; 
  private String email;
public Employee(int empId, String name, String dept, String email) {
this.empId = empId;
  this.name = name; 
  this.dept = dept; 
  this.email = email;
}
public int getEmpId() { 
  return empId; 
}
public String getName() {
  return name; 
}
public String getDept() { 
  return dept; 
}
public String getEmail() {
  return email;
}

public void setName(String name) {
  this.name = name; 
} 
  public void setDept(Stringdept) { 
    this.dept = dept; 
  }
public void setEmail(String email) { 
  this.email = email;
}

@Override
public String toString() {
  return "Employee#" +empId + " [" + name + ", " +dept + ", " + email + "]";
}
}
